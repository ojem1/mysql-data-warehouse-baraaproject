  SELECT '>> Truncated the clean_erp_cust_az12 Table>>' as t_display;
  SELECT '>> Inserting cleaned data into the clean_erp_cust_az12 Table>>' as d_display;
  INSERT INTO clean_erp_cust_az12 (cid, bdate, gen)
  SELECT * FROM
  (
  SELECT 
  CASE WHEN cid like 'NAS%' THEN SUBSTR(CID, 4)
   Else cid
  End cid, 
  CASE WHEN bdate > CURRENT_DATE() THEN NULL
   ELSE bdate
  END bdate,
  CASE WHEN UPPER(TRIM(gen)) = 'M' Then 'Male'
   WHEN UPPER(TRIM(gen)) = 'F' Then 'Female'
   WHEN TRIM(gen) = '' OR gen IS NULL Then 'n/a'
   ELSE gen
  End gen
  FROM raw_erp_cust_az12
  ) fil_tab
  ;
  SELECT '>> Data Insert Complete>>' as d_display;
  -- Inserting the erp_loc
  SELECT '>> Truncating the clean_erp_loc_a101 Table>>' as t_display;
  TRUNCATE TABLE clean_erp_loc_a101;
  SELECT '>> Truncated the clean_erp_loc_a101 Table>>' as t_display;
  SELECT '>> Inserting cleaned data into the clean_erp_loc_a101 Table>>' as d_display;
  INSERT INTO clean_erp_loc_a101(cid, cntry)
  SELECT * FROM (
  SELECT
  CASE WHEN SUBSTRING(cid,3,1) = '-' THEN REPLACE(cid,'-','')
   ELSE cid
  END cid, 
  CASE WHEN cntry in ('USA', 'US') THEN 'United States'
   WHEN TRIM(cntry) = '' OR cntry IS NULL THEN 'n/a'
   WHEN TRIM(cntry) = 'DE' THEN 'Germany'
   ELSE TRIM(cntry)
  END cntry
  FROM raw_erp_loc_a101) AS new_loc_tab;
  SELECT '>> Data Insert Complete>>' as d_display;
 
  -- Inserting the erp_rx_cat
 
  SELECT '>> Truncating the clean_erp_rx_cat Table>>' as t_display;
  TRUNCATE TABLE clean_erp_loc_a101;
  SELECT '>> Truncated the clean_erp_rx_cat Table>>' as t_display;
  SELECT '>> Inserting cleaned data into the clean_erp_rx_cat Table>>' as d_display;
 
  INSERT INTO clean_erp_rx_cat_gtv2(
  id,
  cat,
  subcat,
  maintenance
  )
  SELECT 
  id,
  cat,
  subcat,
  maintenance
  FROM raw_erp_rx_cat_gtv2;
  SELECT '>> Data Insert Complete>>' as d_display;
 
